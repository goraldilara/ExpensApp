
@{
    Layout = null;
}
@model List<CommonArea.FormCommon>
<!DOCTYPE html>

<html>
<head>
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
    <link href="~/Content/css/GridSheetforFillExpenseFormPage.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width" />
    <title>StaffReportsPage</title>
</head>
<body>
    <div>
        <h1 align="center">RAPORLAR</h1>
    </div>
    <div class="container">
        <div class="row col-md-6 col-md-offset-2 custyle">
            <table class="table table-striped custab">
                <tr>
                    <td>Form Tarihi</td>
                    <td>Toplam Masraf Tutarı</td>
                    <td>Onay/Ödendi Durumu</td>
                </tr>
                @{
                    var list = Model;
                    foreach (var item in list)
                    {
                        <tr>
                            <td>@item.Date.ToString("dd/MM/yyyy")</td>
                            <td>@item.TotalAmount</td>
                            <td>@item.Status</td>
                        </tr>
                    }
                }
            </table>
        </div>
    </div>
</body>
</html>
